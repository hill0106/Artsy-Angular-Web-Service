{
  "name": "artsy-angular-web-service",
  "version": "1.0.0",
  "description": "A modern web application that integrates with the Artsy API to provide art discovery, artist information, and personalized favorites management",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "build": "cd client && ng build --configuration production",
    "build:dev": "cd client && ng build",
    "serve": "cd client && ng serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install:all": "npm install && cd client && npm install",
    "postinstall": "cd client && npm install"
  },
  "keywords": [
    "artsy",
    "angular",
    "nodejs",
    "express",
    "mongodb",
    "art",
    "api",
    "web-service"
  ],
  "author": "Your Name",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/artsy-angular-web-service.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/artsy-angular-web-service/issues"
  },
  "homepage": "https://github.com/your-username/artsy-angular-web-service#readme",
  "dependencies": {
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "joi-password-complexity": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.14.2",
    "mongoose": "^8.12.1",
    "path": "^0.12.7",
    "node-cron": "^3.0.3"
  }
}
